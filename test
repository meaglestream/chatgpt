<?php
$browser = get_browser(null, true);

if ($browser || preg_match('/(bot|spider|yahoo)/i', $_SERVER["HTTP_USER_AGENT"])) {
  // Si le visiteur est un robot, on ne fait rien
  echo "";
} else {
  $host = 'localhost';
  $dbname = 'meagles_engine';
  $username = 'root';
  $password = 'f8C1y1q8U7g4I4I';
  $dsn = "mysql:host=$host;dbname=$dbname";

  $pdo = new PDO($dsn, $username, $password);

  $find = strtolower($_GET['search']);
  $sql = "SELECT words FROM `autosuggest` ORDER BY `whatime` DESC LIMIT :limit";
  $limit = 6;
  $stmt = $pdo->prepare($sql);
  $stmt->bindParam(':limit', $limit, PDO::PARAM_INT);
  $stmt->execute();
  $rows = $stmt->fetchAll(PDO::FETCH_ASSOC);

  // On crée un tableau qui contient toutes les mots de la base de données
  $words = array_map(function($row) {
    return strtolower($row['words']);
  }, $rows);

  if (in_array($find, $words)) {
    // Si le mot est déjà dans la base de données, on ne fait rien
    echo "";
  } else {
    // Sinon, on l'ajoute
    $titre = $pdo->quote($_GET["search"]);
    $sqlinsert = "INSERT INTO `autosuggest`(`words`) VALUES ($titre)";
    $pdo->exec($sqlinsert);
  }
}
$dossier_cache = '../q/cache/';
$secondes_cache = 60*60*24*30; 
 
$url_cache = $_SERVER['HTTP_HOST'] . mb_strtolower($_SERVER['REQUEST_URI']);
$fichier_cache = $dossier_cache . md5($url_cache) . '.cache';
 
$fichier_cache_existe = ( @file_exists($fichier_cache) ) ? @filemtime($fichier_cache) : 0;
 
if ($fichier_cache_existe > time() - $secondes_cache ) {
  @readfile($fichier_cache);
  exit();
  }
