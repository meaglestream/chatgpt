	<script language="javascript">
let inputElt = document.getElementById('moteur');
let bttn = document.getElementById('buttonclear');
  if (inputElt.value != "") {
    bttn.style.display = 'block';
  } else {
    bttn.style.display = 'none';
  } 
function isCharSet() {
  if (inputElt.value != "") {
    bttn.style.display = 'block';
  } else {
    bttn.style.display = 'none';
  }  
}
	</script>
	<script language="javascript">
let header = document.querySelector("#header");
let div = document.querySelector("#entre-form");
let span = document.querySelector("#burger");

let lastScrollValue = 0;

document.addEventListener('scroll',() => {
		let top  = document.documentElement.scrollTop;
    if(lastScrollValue < top) {
    	header.classList.add("hidden");
		div.classList.add("hidden");
		header.classList.remove("animhead");
		div.classList.remove("animhead");
    } else {
    	header.classList.remove("hidden");
		div.classList.remove("hidden");
		header.classList.add("animhead");
		div.classList.add("animhead");
    }
    lastScrollValue = top;
});
	</script>

	<script type="text/javascript">
var btn = $('#buttontop');
$(window).scroll(function() {
  if ($(window).scrollTop() > 300) {
    btn.addClass('show');
  } else {
    btn.removeClass('show');
  }
});

btn.on('click', function(e) {
  e.preventDefault();
  $('html, body').animate({scrollTop:0}, '300');
});
	</script>
	
	<script>
// duration of scroll animation
var scrollDuration = 1000;
// paddles
var leftPaddle = document.getElementsByClassName('left-paddle');
var rightPaddle = document.getElementsByClassName('right-paddle');
var nb_element_item = $(".short-cdr").length;
// get items dimensions
var itemsLength = $('.short-cdr').length;
var itemSize = $('.short-cdr').outerWidth(true);
// get some relevant size for the paddle triggering point
var paddleMargin = 20;


// get wrapper width
var getMenuWrapperSize = function() {
	return $('.menu-wrapper').outerWidth();
}
var menuWrapperSize = getMenuWrapperSize();
// the wrapper is responsive
$(window).on('resize', function() {
	menuWrapperSize = getMenuWrapperSize();
});
// size of the visible part of the menu is equal as the wrapper size 
var menuVisibleSize = menuWrapperSize;

// get total width of all menu items
var getMenuSize = function() {
	return itemsLength * itemSize;
};
var menuSize = getMenuSize();
// get how much of menu is invisible
var menuInvisibleSize = menuSize - menuWrapperSize;

// get how much have we scrolled to the left
var getMenuPosition = function() {
	return $('.menu').scrollLeft();
};

// finally, what happens when we are actually scrolling the menu
$( window ).on( "load", function() {
		if (menuInvisibleSize <= 0) {
		$(leftPaddle).addClass('hiddenpaddle');
		$(rightPaddle).addClass('hiddenpaddle');
}}); 
	
$( window ).on( "load", function() {
		if (itemsLength == 0) {
		$(leftPaddle).addClass('hiddenpaddle');
		$(rightPaddle).addClass('hiddenpaddle');
}}); 
	
$('.menu').on('scroll', function() {

	// get how much of menu is invisible
	menuInvisibleSize = menuSize - menuWrapperSize;
	// get how much have we scrolled so far
	var menuPosition = getMenuPosition();

	var menuEndOffset = menuInvisibleSize - paddleMargin;

	// show & hide the paddles 
	// depending on scroll position
	if (menuPosition <= paddleMargin) {
		$(leftPaddle).addClass('hiddenpaddle');
		$(rightPaddle).removeClass('hiddenpaddle');
	} else if (menuPosition < menuEndOffset) {
		// show both paddles in the middle
		$(leftPaddle).removeClass('hiddenpaddle');
		$(rightPaddle).removeClass('hiddenpaddle');
	} else if (menuPosition >= menuEndOffset) {
		$(leftPaddle).removeClass('hiddenpaddle');
		$(rightPaddle).addClass('hiddenpaddle');
}


});
 

// scroll to left
$(rightPaddle).on('mouseover', function() {
	$('.menu').animate( { scrollLeft: '+=515'}, scrollDuration);
});
$(rightPaddle).on('click', function() {
	$('.menu').animate( { scrollLeft: '+=515'}, scrollDuration);
});
// scroll to right
$(leftPaddle).on('mouseover', function() {
	$('.menu').animate( { scrollLeft: '-=515' }, scrollDuration);
});$(leftPaddle).on('click', function() {
	$('.menu').animate( { scrollLeft: '-=515' }, scrollDuration);
});
    </script>
		
