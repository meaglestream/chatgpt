	 try {
        $bdd = new PDO('mysql:host=localhost;dbname=meagles_engine', 'root', 'f8C1y1q8U7g4I4I');
  }
   catch (Exception $e) {
        die('Erreur : '.$e->getMessage());
  }
    $reponse = $bdd->prepare('SELECT word FROM correctindex WHERE word LIKE :req');
    $reponse->execute(array('req' => '%'.$_GET['req'].'%'));
    $listeto = array();

    while($donnees = $reponse->fetch()) {
        $a = count($listeto);
    // formatage de l'affichage des données de la liste
        $listeto[$a] = $donnees['word'];
    }

$wordsto  = $listeto;
$wordtest = -1;
foreach ($wordsto as $wordto) {
	$wordto = ucfirst(mb_strtolower($wordto));
    $leven = levenshtein(ucfirst(mb_strtolower($searchpage)), $wordto);
    if ($leven == 0) {
        $closeword = $wordto;
        $wordtest = 0;
        break;
    }
    if ($leven <= $wordtest || $wordtest < 0) {
        $closeword  = $wordto;
        $wordtest = $leven;
    }
}
	 
	  try {
        $bdd = new PDO('mysql:host=localhost;dbname=meagles_engine', 'root', 'f8C1y1q8U7g4I4I');
  }
   catch (Exception $e) {
        die('Erreur : '.$e->getMessage());
  }
    $reponse = $bdd->prepare('SELECT word FROM correctindex WHERE word LIKE :raq');
    $reponse->execute(array('raq' => '%'.$_GET['raq'].'%'));
    $liste = array();
	
    while($donnees = $reponse->fetch()) {
        $a = count($liste);
    // formatage de l'affichage des données de la liste
        $liste[$a] = $donnees['word'];
    }

$words  = $liste;
$shortest = -1;
foreach ($words as $word) {
	$word = ucfirst(mb_strtolower($word));
    $lev = levenshtein(ucfirst(mb_strtolower($searchpage)), $word);
    if ($lev == 0) {
        $closest = $word;
        $shortest = 0;
        break;
    }
    if ($lev <= $shortest || $shortest < 0) {
        $closest  = $word;
        $shortest = $lev;
    }
}

	
