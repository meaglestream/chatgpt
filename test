<!-- Matomo -->
<script>
  var _paq = window._paq = window._paq || [];
  /* tracker methods like "setCustomDimension" should be called before "trackPageView" */
  _paq.push(['trackPageView']);
  _paq.push(['enableLinkTracking']);
  (function() {
    var u="https://french-interface.com/statistiques/";
    _paq.push(['setTrackerUrl', u+'matomo.php']);
    _paq.push(['setSiteId', '3']);
    var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
    g.async=true; g.src=u+'matomo.js'; s.parentNode.insertBefore(g,s);
  })();
</script>
<!-- End Matomo Code -->
	<script language="javascript">
		// on sélectionne les liens de pages qui joueront l'animation
const liens = document.querySelectorAll('button, a')


for(i=0; i< liens.length; i++){
    let lien = liens[i]

    // on écoute le clic sur ces liens
    lien.addEventListener('click' , function(event){

        //on empêche le lien de nous diriger vers une autre page
        event.preventDefault();
lienclic = event.target.href
   setTimeout(function(){window.location.href = lienclic}, 400)     //on ajoute alors la classe "active" pour ajouter le fondu au noir
        transition.classList.add("active")
		setTimeout(function(){transition.classList.remove("active")}, 10400) 
    })
	
	
}
            
        
		</script>

