<script language="javascript">
function displayNavInfo(text){
    if(text){
        //Détection du navigateur
        is_ie = (navigator.userAgent.toLowerCase().indexOf("msie") != -1) && (navigator.userAgent.toLowerCase().indexOf("opera") == -1);
         
        //Création d'une div provisoire
        var NavInfo = document.createElement('div');
        NavInfo.style.position = 'fixed';
		NavInfo.style.zIndex = '1020';
        document.onmousemove = function(e){
            x = (!is_ie ? e.pageX-window.pageXOffset : event.x+document.body.scrollLeft);
            y = (!is_ie ? e.pageY-window.pageYOffset : event.y+document.body.scrollTop);
            NavInfo.style.left = x+15+'px';
            NavInfo.style.top = y+15+'px';
        }
        NavInfo.id = 'NavInfo';
        NavInfo.innerHTML = text;
        document.body.appendChild(NavInfo);
    }
    else{
        document.onmousemove = '';
        document.body.removeChild(document.getElementById('NavInfo'));
    }
}
</script>

<script language="javascript">
function displayLinkInfo(text){
    if(text){
        //Détection du navigateur
        is_ie = (navigator.userAgent.toLowerCase().indexOf("msie") != -1) && (navigator.userAgent.toLowerCase().indexOf("opera") == -1);
         
        //Création d'une div provisoire
        var LinkInfo = document.createElement('div');
        LinkInfo.style.position = 'fixed';
		LinkInfo.style.zIndex = '1020';
        document.onmousemove = function(e){
            x = (!is_ie ? e.pageX-window.pageXOffset : event.x+document.body.scrollLeft);
            y = (!is_ie ? e.pageY-window.pageYOffset : event.y+document.body.scrollTop);
            LinkInfo.style.left = x+-100+'px';
            LinkInfo.style.top = y+15+'px';
        }
        LinkInfo.id = 'LinkInfo';
        LinkInfo.innerHTML = text;
        document.body.appendChild(LinkInfo);
    }
    else{
        document.onmousemove = '';
        document.body.removeChild(document.getElementById('LinkInfo'));
    }
}
</script>
	
